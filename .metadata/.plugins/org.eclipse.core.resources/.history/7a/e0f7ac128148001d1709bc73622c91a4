package asalSayiDemo;

public class Main {

	public static void main(String[] args) {
		int sayi = 12;
		int bolum = 0;

		for (int i = 2; i < sayi; i++) {
			if (sayi % i == 0) {
				bolum += 1;
			}
		}

		if (bolum == 0) {
			System.out.println("Sayi Asaldir.");
		}else {
			System.out.println("Sayi Asal Degildir.");
		}
	}

}
