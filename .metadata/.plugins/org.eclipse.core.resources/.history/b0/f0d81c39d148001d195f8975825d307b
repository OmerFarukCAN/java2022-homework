package mukemmelSayiDemo;

public class Main {
	public static void main(String[] args) {

		int bolum = 0;

		for (int i = 9; i > 2; i--) {
			for (int j = 2; j < 10; j++) {
				if (i % j == 0) {
					bolum += 1;	
				}
			}
			if (bolum > 1) {
				System.out.println("Sayi asal degildir." + i);
				bolum = 0;
			}else {
				System.out.println("Sayi asaldir." + i);
			}
		}
		
		System.out.println(2 % 3);
	}
}
