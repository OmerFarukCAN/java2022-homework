package mukemmelSayiDemo;

import java.util.ArrayList;
import java.util.List;

public class Main {
	public static void main(String[] args) {

		int bolum = 0;
		List<Integer> mukemmels=new ArrayList<Integer>(); // array liste ekle dÄ±sarda for dongusu yap
		mukemmels.add(2);
		System.out.println("Sayi asaldir: 2");
		
		for (int i = 3; i < 9; i++) {
			for (int j = 2; j < 9; j++) {
				if (i % j == 0) {
					bolum += 1;	
				}
			}
			
			if (bolum > 1) {
				System.out.println("Sayi asal degildir: " + i);
				bolum = 0;
			}else {
				System.out.println("Sayi asaldir: " + i);
				//mukemmels.add(i);
			}
		}
		
		
	}
}
