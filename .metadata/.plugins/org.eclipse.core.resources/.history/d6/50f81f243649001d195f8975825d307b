package mukemmelSayiDemo;

import java.util.ArrayList;
import java.util.List;

public class Main {
	public static void main(String[] args) {

		int bolum = 0;
		List<Integer> mukemmels=new ArrayList<Integer>(); // array liste ekle dÄ±sarda for dongusu yap
		mukemmels.add(2);

		for (int i = 3; i < 9; i++) {
			for (int j = 2; j < 9; j++) {
				if (i % j == 0) {
					bolum += 1;	
				}
			}
			System.out.println("Sayi asaldir: " + (i-1));
			
			if (bolum > 1) {
				System.out.println("Sayi asal degildir: " + i);
				bolum = 0;
			}else {
				System.out.println("Sayi asaldir: " + i);
				//mukemmels.add(i);
			}
		}
		
//		for (int i = 1; i <= mukemmels.size(); i++) {
//			System.out.println(i +".Mukemmel sayi: " + ((int)Math.pow(2, mukemmels.get(i)-1)) * 
//					((int)Math.pow(2, mukemmels.get(i)) -1 ));
//		}
	}
}
